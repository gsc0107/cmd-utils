<cmd name="TableToXml">


<options>
	<option name="delimiter" opt="d" longopt="delim" default="\t" type="char">
		<description>Column delimiter</description>
	</option>
	<option name="hidexmlheader" opt="x" longopt="noheader" type="bool">
		<description>Do not print XML header</description>
	</option>
	<option name="ignoreheader" opt="a" longopt="noheader" type="bool">
		<description>first line is not a header</description>
	</option>
	<option name="print_nl_row" opt="r" longopt="newline" type="bool">
		<description>print a new line afer each row</description>
	</option>
	<option name="useheadername" opt="u" longopt="headernames" type="bool">
		<description>Use header names instead of 'td'</description>
	</option>
	<option name="indent" opt="i" longopt="indent" type="bool">
		<description>indent</description>
	</option>
	<option name="cdatalen" opt="c" longopt="cdatalen" type="int" default="1000">
		<description>use CDATA when length of string is greater than cdatalen</description>
	</option>
	<option name="xmlns" opt="n" longopt="xmlns" type="char*">
		<description>xml namespace</description>
	</option>
</options>


</cmd>
