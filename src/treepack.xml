<cmd name="TreePack">


<options>
	<option name="delimiter" opt="d" longopt="delim" default="\t" type="char">
		<description>Column delimiter</description>
	</option>
	<option name="input_from_unique" type="bool" opt="u" longopt="uniq" >
		<description>input is `uniq -c` output</description>
	</option>
	<option name="image_width" type="int" opt="W" longopt="width" default="1000">
		<description>image width</description>
	</option>
	<option name="image_height" type="int" opt="H" longopt="height" default="1000" >
		<description>image height</description>
	</option>
	<option name="output_name" type="char*" opt="o" longopt="out">
		<description>Output file</description>
	</option>
</options>


</cmd>
